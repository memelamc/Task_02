@page
@model Task_02.Pages.UserData.UserDataModel
@{
    ViewData["Title"] = "User Data From JSON Object";
}
    <h1> User Data</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Name
                </th>

                <th>
                    Surname
                </th>
                <th>
                    Email Address
                </th>

                <th>
                    Gender
                </th>
                <th>
                    IP Address
            </tr>
            @foreach (var user in Model.Users)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => user.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => user.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => user.Surname)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => user.EmailAddress)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => user.Gender)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => user.IpAddress)
                    </td>
                </tr>
            }
        </tbody>
    </table>